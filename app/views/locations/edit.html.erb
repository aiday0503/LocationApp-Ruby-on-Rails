<h1> Editing <%= @location.name %></h1>
<form action="/locations/<%= @location.id %>" method="post">
<input type="hidden" name="method" value="patch">
<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
Name:
<input type="text" name="name" value="<%= @location.name %>"> <br>
Address:
<input type="text" name="street" value="<%= @location.street %>"><br>
Zip Code:
<input type="text" name="zipcode" value="<%= @location.zipcode %>"><br>
<input type="submit" value="Update Branch">
</form>
